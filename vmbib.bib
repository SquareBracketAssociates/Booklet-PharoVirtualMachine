@inproceedings{Choi89a,
  author = {Choi, B. J. and DeMillo, R. A. and Krauser, E. W. and Martin, R. J. and Mathur, A. P. and Offutt, A. J. and Pan, H. and Spafford, E. H.},
  title = {The Mothra Tool Set (Software Testing)},
